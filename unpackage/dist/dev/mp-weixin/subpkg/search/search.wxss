@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}
.sugg-list {
  padding: 0 5px;
}
.sugg-list .sugg-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 12px;
  padding: 13px 0;
  border-bottom: 1px solid #efefef;
}
.sugg-list .sugg-item .people-name, .sugg-list .sugg-item .people-fieldname {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.Fee {
  text-align: center;
  color: red;
}
.danjia, .zongmushu, .zongqianshu, .pay {
  margin-left: 10px;
}
.info {
  text-align: center;
  background-color: red;
}

